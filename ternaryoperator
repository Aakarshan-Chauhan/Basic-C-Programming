// C program to find largest among two  
// numbers using ternary operator 
  
#include <stdio.h> 
#define max(a, b) ((a)>(b))?(a):(b)
#define min(a, b) ((a)<(b))?(a):(b)

int main() 
{ 
    // variable initialization 
    int n1 = 5, n2 = 10, n3 = -12; 
    
    // Print the largest number 
    printf("Largest number between %d, %d and %d is %d. ", n1, n2, n3, max(n1, max(n2, m3))); 
    // Print the smallest number
    printf("Smallest number between %d , %d and %d is %d", n1, n2, n3, min(n1, min(n2, m3)));
    return 0; 
} 
